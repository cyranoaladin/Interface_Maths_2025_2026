{
  "ci": {
    "collect": {
      "startServerCommand": "",
      "url": [
        "http://127.0.0.1:4173/index.html",
        "http://127.0.0.1:4173/EDS_premiere/index.html",
        "http://127.0.0.1:4173/EDS_terminale/index.html",
        "http://127.0.0.1:4173/Maths_expertes/index.html"
      ],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.85
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.95
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "total-byte-weight": [
          "error",
          {
            "maxNumericValue": 550000
          }
        ],
        "script-treemap-data": "off"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./.lhci_reports"
    }
  }
}
