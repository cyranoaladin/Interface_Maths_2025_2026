{
  "ci": {
    "collect": {
      "url": [
        "http://127.0.0.1:8008/content/index.html"
      ],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop",
        "emulatedFormFactor": "desktop",
        "chromeFlags": "--headless=new --no-sandbox"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
